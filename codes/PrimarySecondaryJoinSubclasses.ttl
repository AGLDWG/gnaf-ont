@prefix : <http://gnafld.net/def/gnaf/> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix dct: <http://purl.org/dc/terms/> .
@prefix vann: <http://purl.org/vocab/vann/> .
@prefix gnaf: <http://gnafld.net/def/gnaf/> .
@base <http://gnafld.net/def/gnaf> .

<http://gnafld.net/def/gnaf/AutoPrimarySecondaryJoinSubclassCollection> a skos:Collection ;
    skos:member
      :AutoPrimarySecondaryJoin ,
      :ManualPrimarySecondaryJoin
    ;
    skos:prefLabel "Primary Secondary Join Collection" .

# This ensures that an instance of a Primary Secondary Join cannot be of more than one subclass
[
    a owl:AllDisjointClasses ;
    owl:members (
          :AutoPrimarySecondaryJoin ,
          :ManualPrimarySecondaryJoin
      ) ;
].

:AutoPrimarySecondaryJoin rdfs:subClassOf gnaf:PrimarySecondaryJoin , skos:Concept ;
    skos:prefLabel "Auto"@en ;
    skos:definition "Automtically matched Primary & Secondary, both parent & child have same root Address."@en ;
    skos:altLabel "1"@en ;
    dct:source "G-NAF Data Product Description, November 2017"^^xsd:string ;
    dct:countributor <http://orcid.org/0000-0002-8742-7730> ;
    rdfs:isDefinedBy <http://gnafld.net/def/gnaf/> .

:ManualPrimarySecondaryJoin rdfs:subClassOf gnaf:PrimarySecondaryJoin , skos:Concept ;
    skos:prefLabel "Manual"@en ;
    skos:definition "Manually generated link, may or may not have same root Address"@en ;
    skos:altLabel "2"@en ;
    dct:source "G-NAF Data Product Description, November 2017"^^xsd:string ;
    dct:countributor <http://orcid.org/0000-0002-8742-7730> ;
    rdfs:isDefinedBy <http://gnafld.net/def/gnaf/> .
