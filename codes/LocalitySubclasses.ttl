<http://gnafld.net/def/gnaf/LocalitySubclassCollection> a skos:Collection ;
    skos:member
        :AliasOnlyLocality ,
        :DistrictLocality ,
        :GazetedLocality ,
        :HundredLocality ,
        :ManuallyValidatedLocality ,
        :TopographicLocality ,
        :UnofficialSuburbLocality ,
        :UnofficialTopographicFeatureLocality
    ;
    skos:prefLabel "Locality Subclass Collection" .

# This ensures that an instance of a Locality cannot be of more than one subclass
[
    rdf:type owl:AllDisjointClasses ;
    owl:members (
        :AliasOnlyLocality
        :DistrictLocality
        :GazetedLocality
        :HundredLocality
        :ManuallyValidatedLocality
        :TopographicLocality
        :UnofficialSuburbLocality
        :UnofficialTopographicFeatureLocality
      ) ;
].

