<http://gnafld.net/def/gnaf/AliasSubclassCollection> a skos:Collection ;
    skos:member
      :SynonymAlias ,
      :ContributorDefinedAlias ,
      :AlternativeLocalityAlias ,
      :RangedAddressAlias ,
      :LevelDuplicationAlias ,
      :FlatNumberNoFirstSuffixCorrelationAlias ,
      :MaintenanceReferenceAlias ,
      :SpatiallyRelatedAlias ,
      :AlternativeAlias ,
      :LocalityAlias ,
      :StreetLocalityAlias
    ;
    skos:prefLabel "Alias Subclass Collection" .

# This ensures that an instance of an Alias cannot be of more than one subclass
[
  rdf:type owl:AllDisjointClasses ;
  owl:members (
        :SynonymAlias
        :ContributorDefinedAlias
        :AlternativeLocalityAlias
        :RangedAddressAlias
        :LevelDuplicationAlias
        :FlatNumberNoFirstSuffixCorrelationAlias
        :MaintenanceReferenceAlias
        :SpatiallyRelatedAlias
        :AlternativeAlias
        :LocalityAlias
        :StreetLocalityAlias
    ) ;
].

