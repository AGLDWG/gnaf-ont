    skos:altLabel "SYN" ;
    dct:source "G-NAF Data Product Description, November 2017: ADDRESS_ALIAS_TYPE_AUT"^^xsd:string ;
    dct:contributor <http://orcid.org/0000-0002-8742-7730> ;
    rdfs:isDefinedBy <http://gnafld.net/def/gnaf/> .

