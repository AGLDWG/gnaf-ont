:MB2016 rdf:type owl:Class ;
        rdfs:subClassOf :MeshBlock ;
        dcterms:contributor <http://orcid.org/0000-0002-8742-7730> ;
        dcterms:source "G-NAF Data Product Description, November 2017" ;
